<p id="notice"><%= notice %></p>

<h1>Listing Publishing Companies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Prefecture</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @publishing_companies.each do |publishing_company| %>
      <tr>
        <td><%= publishing_company.name %></td>
        <td><%= publishing_company.description %></td>
        <td><%= publishing_company.prefecture_id %></td>
        <td><%= link_to 'Show', publishing_company %></td>
        <td><%= link_to 'Edit', edit_publishing_company_path(publishing_company) %></td>
        <td><%= link_to 'Destroy', publishing_company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Publishing company', new_publishing_company_path %>
